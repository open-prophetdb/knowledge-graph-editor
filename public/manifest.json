{
  "name": "Knowledge Graph Editor",
  "version": "1.0",
  "description": "An extension to curate findings from the web and publications into a knowledge graph",
  "manifest_version": 3,
  "background": {
    "service_worker": "static/js/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "static/css/content.css"
      ],
      "js": [
        "static/js/content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "host_permissions": [],
  "web_accessible_resources": [
    {
      "resources": [
        "/images/app.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    },
    {
      "resources": [
        "insert.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "/images/app.png",
      "32": "/images/app.png",
      "48": "/images/app.png",
      "128": "/images/app.png"
    },
    "default_title": "Knowledge Graph Editor"
  },
  "icons": {
    "16": "/images/app.png",
    "32": "/images/app.png",
    "48": "/images/app.png",
    "128": "/images/app.png"
  }
}